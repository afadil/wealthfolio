{
  "name": "@wealthfolio/ui",
  "version": "2.0.0",
  "type": "module",
  "description": "Wealthfolio UI components based on shadcn/ui and Tailwind CSS",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./styles": {
      "import": "./dist/styles.css"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "wealthfolio",
    "ui",
    "components",
    "shadcn",
    "tailwind",
    "react",
    "components"
  ],
  "author": "Wealthfolio Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/afadil/wealthfolio.git",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/afadil/wealthfolio/issues"
  },
  "homepage": "https://github.com/afadil/wealthfolio/tree/main/packages/ui#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:quiet": "eslint . --quiet",
