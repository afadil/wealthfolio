{
  "name": "@wealthfolio/ui",
  "version": "3.0.0-beta.2",
  "type": "module",
  "description": "Wealthfolio UI components based on shadcn/ui and Tailwind CSS",
  "main": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./chart": {
      "import": "./dist/chart.js",
      "types": "./dist/src/chart.d.ts"
    },
    "./styles": {
      "import": "./dist/styles.css"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "wealthfolio",
    "ui",
    "components",
    "shadcn",
    "tailwind",
    "react",
    "components"
  ],
  "author": "Wealthfolio Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/afadil/wealthfolio.git",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/afadil/wealthfolio/issues"
  },
  "homepage": "https://github.com/afadil/wealthfolio/tree/main/packages/ui#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup && pnpm run build:types",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:quiet": "eslint . --quiet",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "build:types": "tsc -p tsconfig.json"
  },
