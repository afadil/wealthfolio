{
  "name": "@wealthfolio/addon-sdk",
  "version": "1.1.0",
  "description": "TypeScript SDK for building Wealthfolio addons with enhanced functionality and type safety",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./permissions": {
      "import": "./dist/permissions.js",
      "types": "./dist/permissions.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "wealthfolio",
    "addon",
    "plugin",
    "sdk",
    "typescript",
    "financial",
    "portfolio"
  ],
  "author": "Wealthfolio Team",
  "license": "MIT",
  "homepage": "https://wealthfolio.app/addons",
  "repository": {
    "type": "git",
    "url": "https://github.com/afadil/wealthfolio.git",
    "directory": "packages/addon-sdk"
  },
  "bugs": {
    "url": "https://github.com/afadil/wealthfolio/issues"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit",
    "dev-server": "node dev-server.js",
    "addon": "node cli.js",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "commander": "^9.0.0",
    "express": "^4.18.0",
    "cors": "^2.8.0",
    "chokidar": "^3.5.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 